# Plaid API Credentials
# Get these from: https://dashboard.plaid.com/
PLAID_CLIENT_ID=your_plaid_client_id_here
PLAID_SECRET=your_plaid_secret_here
PLAID_ENV=sandbox
# After completing Plaid Link flow, add your access token:
PLAID_ACCESS_TOKEN=

# Experian API Credentials
# Get these from: https://developer.experian.com/
EXPERIAN_CLIENT_ID=your_experian_client_id_here
EXPERIAN_CLIENT_SECRET=your_experian_client_secret_here
EXPERIAN_ENV=sandbox
# Your Experian Developer Portal login credentials (required for OAuth)
EXPERIAN_USERNAME=your_experian_username
EXPERIAN_PASSWORD=your_experian_password

# Consumer Information (for Experian API)
# In production, this would come from user input
CONSUMER_FIRST_NAME=John
CONSUMER_LAST_NAME=Doe
CONSUMER_SSN=666112222
CONSUMER_DOB=1980-01-01
CONSUMER_ADDRESS_LINE1=123 Main St
CONSUMER_CITY=New York
CONSUMER_STATE=NY
CONSUMER_ZIP=10001
